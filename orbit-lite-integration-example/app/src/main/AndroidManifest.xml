<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.element.example">

    <uses-permission android:name="com.element.palm.portal.READ_PROVIDER" />
    <uses-permission android:name="com.element.palm.portal.WRITE_PROVIDER" />

    <application
        android:name=".ElementSDKExampleApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".UserActivity"
            android:label="@string/enroll_user"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="com.element.EAK"
            android:value="[YOUR_EAK]" />

    </application>

</manifest>