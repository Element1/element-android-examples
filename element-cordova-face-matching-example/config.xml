<?xml version='1.0' encoding='utf-8'?>
<widget id="com.element.facematchdemo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Face Matching Sdk Demo</name>
    <description>
        A demo application using Element Face Matching SDK plugin for Cordova
    </description>
    <author email="devsupport@discoverelement.com" href="http://www.discoverelement.com">
        Element
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="19" />
        <allow-intent href="market:*" />
        <config-file mode="merge" parent="/*" target="res/values/strings.xml">
            <string name="fm_url">FM URL GOES HERE</string>
            <string name="api_key">API KEY GOES HERE</string>
        </config-file>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="element-face-matching-sdk" spec="/path/to/element-face-matching-sdk" />
    <engine name="android" spec="^7.1.4" />
</widget>
